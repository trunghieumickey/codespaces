{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "ubuntu-22.04" }
	},
	"settings": {
		"files.exclude": {
			"**/.devcontainer": true
		},
		"workbench.colorTheme": "Default Dark+",
		"files.autoSave": "off",
		"window.menuBarVisibility": "classic"
	},
	"postCreateCommand": "sudo rm -rf /workspaces/codespaces/.git",
	"extensions": [],
	"remoteUser": "vscode"
}
